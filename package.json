{
  "name": "remix-cloudflare-template-test",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:db": "prisma generate --no-engine",
    "build:remix": "remix vite:build  --sourcemapClient",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "db:format": "prisma format",
    "db:generate": "prisma generate --no-engine",
    "db:seed": "prisma db seed",
    "db:setup": "prisma generate --no-engine && prisma migrate deploy && prisma db seed",
    "format": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "prisma:studio": "prisma studio",
    "start": "wrangler pages dev ./build/client",
    "setup": "run-s npm:install db:generate",
    "typecheck": "tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@conform-to/react": "^1.0.2",
    "@prisma/client": "^5.10.2",
    "@prisma/extension-accelerate": "^1.0.0",
    "@remix-run/cloudflare": "^2.8.0",
    "@remix-run/cloudflare-pages": "^2.8.0",
    "@remix-run/react": "^2.8.0",
    "bcryptjs": "^2.4.3",
    "isbot": "^4.1.0",
    "miniflare": "^3.20231030.4",
    "pnpm": "^8.15.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-h5-audio-player": "^3.9.1",
    "tsx": "^4.7.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.8.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.24.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}